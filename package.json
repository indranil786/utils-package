{
  "name": "@fastjobsio/fastjobs-utils",
  "version": "1.3.0",
  "description": "A private utility library for fastjobs.io",
  "main": "dist/index.js",
  "scripts": {
    "test": "exit 0",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "pre-commit": "lint-staged --allow-empty",
    "lint": "eslint .",
    "prepare": "husky install",
    "format:check": "prettier --check .",
    "format:write": "prettier --write .",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FastJobsio/fastjobs-utils.git"
  },
  "keywords": [
    "fastjobs.io",
    "utils",
    "helperfunction"
  ],
  "author": "Indranil Kar (indranil@fastjobs.io)",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/FastJobsio/fastjobs-utils/issues"
  },
  "homepage": "https://github.com/FastJobsio/fastjobs-utils#readme",
  "devDependencies": {
    "@types/amqplib": "^0.10.1",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.23.0",
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "lint-staged": {
    "**/*.{js,ts}": [
      "npx eslint --fix"
    ],
    "**/*.{js,ts,json}": [
      "npx prettier --write"
    ]
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.305.0",
    "@aws-sdk/client-ses": "^3.303.0",
    "amqplib": "^0.10.3"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  }
}
